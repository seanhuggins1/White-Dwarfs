%input state as [density, mass]
function dstatedr = dwarfrk(state, radius, param)

    %get state variables
    mass = state(1)
     = state(2)
    

    gamma = @(x) (x^2)/(3*(1+x^2)^(1/2));
    
  
    
    dpdr = -(((mass)*density)/((gamma(density^(1/3)))*(radius^2)));

    dmdr = (radius^2)*density;
    
    %Set dstate/dr
    dstatedr = [dpdr, dmdr];

    
end

